#include <stdio_ext.h>
#include <string.h>
#define Max 50
#define ID_LENGHTH 15
#define NAME_LENGHTH 50

typedef struct HoSo
{
  char id[ID_LENGHTH];
  char name[NAME_LENGHTH];
